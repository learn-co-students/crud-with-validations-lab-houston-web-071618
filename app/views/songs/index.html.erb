<h1>Songs:</h1>
<div><%=link_to 'Create Song', new_song_path%></div>
<br>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Year</th>
      <th>Genre</th>
      <th>Artist name</th>
    </tr>
  </thead>
  <tbody>
    
    <%@songs.each do |song|%>
   <tr>
    <td> <%=link_to song.title, song_path(song) %></td>
   
    <td> <%=song.release_year%></td>
    <td> <%=song.genre%></td>
    <td> <%=song.artist_name%></td>
</tr>
    <%end%>
 
  </tbody>
</table>