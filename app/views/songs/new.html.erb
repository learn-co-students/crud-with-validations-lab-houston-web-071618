<p><%= link_to "< Back to All Songs", songs_path %></p>

<h1>Create A Song</h1>
<%= form_for @song do |f| %><br>
  <p>Title: <%= f.text_field :title %><br></p>
  <p>Artist_name: <%= f.text_field :artist_name %><br></p>
  <p>Release_year: <%= f.number_field :release_year %><br></p>
  <p>Genre: <%= f.text_field :genre %><br></p>
  <p>Released Yet?</p>
  <%= radio_button_tag(:released, "true") %>
  <%= label_tag(:released_true, "Yes") %>
  <%= radio_button_tag(:released, "false") %>
  <%= label_tag(:released_false, "No") %>
  <br><br>
  <%= f.submit "Submit Song" %>
<% end %>
  